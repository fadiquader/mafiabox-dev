{
  "name": "mafiabox-dev",
  "version": "0.1.0",
  "author": "Michael Ru",
  "homepage": "./",
  "private": true,
  "main": "public/main.js",
  "dependencies": {
    "concurrently": "^4.0.1",
    "electron-is-dev": "^1.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5",
    "wait-on": "^3.1.0"
  },
  "build": {
    "appId": "com.example.mafiabox-dev",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build -c.extraMetadata.main=build/main.js",
    "preelectron-pack": "npm run-script build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.4",
    "electron-builder": "^20.28.4"
  }
}
